		height: 18rpx;
		position: absolute;
		bottom: 0rpx;
		width: 1000rpx;
		/* background-color: red; */
		overflow: hidden;
		margin-left: -280rpx;
}
.main-top-state-fangwei-animation {
		display: inline-table;
		margin-right: 30rpx;
		width: 33rpx;
		height: 18rpx;
		-webkit-transform: skew(-40deg);
		        transform: skew(-40deg);
		background-color: white;
		-webkit-animation: fangweiAnimation 4.2s infinite linear;
		        animation: fangweiAnimation 4.2s infinite linear;
}
@-webkit-keyframes fangweiAnimation {
from {
			-webkit-transform: translateX(0rpx) skew(-40deg);
			        transform: translateX(0rpx) skew(-40deg);
}
to {
			-webkit-transform: translateX(313rpx) skew(-40deg);
			        transform: translateX(313rpx) skew(-40deg);
}
}
@keyframes fangweiAnimation {
from {
			-webkit-transform: translateX(0rpx) skew(-40deg);
			        transform: translateX(0rpx) skew(-40deg);
}
to {
			-webkit-transform: translateX(313rpx) skew(-40deg);
			        transform: translateX(313rpx) skew(-40deg);
}
}
